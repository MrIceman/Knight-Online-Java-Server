package com.knightonline.shared.network.packet;

import java.util.concurrent.BlockingQueue;
import java.util.concurrent.LinkedBlockingQueue;

import com.knightonline.shared.network.common.MessageInfo;

/**
 * @author Mamaorha
 *
 */
public abstract class PacketHandler
{
	protected BlockingQueue<MessageInfo> requestQueueMap;
	
	public PacketHandler()
	{
		requestQueueMap = new LinkedBlockingQueue<MessageInfo>();
	}
	
	public void addWork(MessageInfo messageInfo)
	{
		requestQueueMap.add(messageInfo);
	}
	
	
}
