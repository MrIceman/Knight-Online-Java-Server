package com.knightonline.shared.network.common;

/**
 * @author Mamaorha
 *
 */
public class Packet
{
	private int opcode;
	private byte[] data;

	public Packet(int opcode)
	{
		this.opcode = opcode;

		data = new byte[0];
	}

	public int getOpcode()
	{
		return opcode;
	}

	public void setOpcode(int opcode)
	{
		this.opcode = opcode;
	}

	public byte[] getData()
	{
		return data;
	}

	public void setData(byte[] data)
	{
		this.data = data;
	}

}
